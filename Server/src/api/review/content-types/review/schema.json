{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review",
    "description": "AI-generated code reviews for pull requests"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "prNumber": {
      "type": "integer",
      "required": true
    },
    "prTitle": {
      "type": "string",
      "maxLength": 512
    },
    "prUrl": {
      "type": "string",
      "maxLength": 1024
    },
    "prAuthor": {
      "type": "string",
      "maxLength": 255
    },
    "headSha": {
      "type": "string",
      "maxLength": 40
    },
    "baseSha": {
      "type": "string",
      "maxLength": 40
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "in_progress", "completed", "failed", "skipped"],
      "default": "pending",
      "required": true
    },
    "aiProvider": {
      "type": "string",
      "maxLength": 50
    },
    "model": {
      "type": "string",
      "maxLength": 100
    },
    "tokensUsed": {
      "type": "integer",
      "default": 0
    },
    "processingTime": {
      "type": "integer"
    },
    "errorMessage": {
      "type": "text"
    },
    "metadata": {
      "type": "json",
      "default": {}
    },
    "completedAt": {
      "type": "datetime"
    },
    "repository": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::repository.repository",
      "inversedBy": "reviews"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review-comment.review-comment",
      "mappedBy": "review"
    },
    "usageLogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::usage-log.usage-log",
      "mappedBy": "review"
    }
  }
}
